{
	"name": "jaden.baptista.dev",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"export": "next export"
	},
	"dependencies": {
		"@types/node": "^14.6.0",
		"@types/react": "^16.9.46",
		"next": "9.5.2",
		"react": "16.13.1",
		"react-dom": "16.13.1",
		"react-no-ssr": "^1.1.0",
		"react-sortablejs": "^6.0.0",
		"sortablejs": "^1.10.2",
		"typescript": "^3.9.7"
	},
	"esLintConfig": {
		"parser": "babel-eslint",
		"parserOptions": {
			"ecmaVersion": 6,
			"sourceType": "module",
			"ecmaFeatures": {
				"jsx": true,
				"modules": true
			}
		},
		"rules": {
			"semi": "error",
			"react/jsx-uses-react": "error",
			"react/jsx-uses-vars": "error"
		},
		"plugins": [
			"react",
			"html"
		],
		"extends": [
			"eslint:all",
			"plugin:react/all"
		],
		"settings": {
			"react": {
				"pragma": "React",
				"version": "16.13.1"
			}
		}
	}
}
